[
  {
    "apiVersion": "gateway.networking.k8s.io/v1",
    "kind": "Gateway",
    "metadata": {
      "name": "test-gateway-1",
      "namespace": "default"
    },
    "spec": {
      "gatewayClassName": "envoy-gateway",
      "listeners": [
        {
          "name": "http",
          "port": 80,
          "protocol": "HTTP"
        }
      ]
    },
    "status": {
      "conditions": [
        {
          "type": "Ready",
          "status": "True",
          "reason": "GatewayReady"
        }
      ]
    }
  },
  {
    "apiVersion": "gateway.networking.k8s.io/v1",
    "kind": "Gateway",
    "metadata": {
      "name": "test-gateway-2",
      "namespace": "production"
    },
    "spec": {
      "gatewayClassName": "envoy-gateway",
      "listeners": [
        {
          "name": "https",
          "port": 443,
          "protocol": "HTTPS",
          "hostname": "api.example.com"
        }
      ]
    },
    "status": {
      "conditions": [
        {
          "type": "Ready",
          "status": "True",
          "reason": "GatewayReady"
        }
      ]
    }
  }
]